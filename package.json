{
  "name": "create-ace-app",
  "license": "MIT",
  "type": "module",
  "version": "0.7.1",
  "author": "https://github.com/acets-team",
  "homepage": "https://github.com/acets-team/create-ace-app#readme",
  "description": "❤️ npx create-ace-app@latest runs a script to create an Ace application!",
  "bin": {
    "create-ace-app": "./cli.js"
  },
  "bugs": {
    "url": "https://github.com/acets-team/create-ace-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acets-team/create-ace-app.git"
  },
  "scripts": {
    "build": "ace build prod && ace sw && vinxi build",
    "build-cli": "ace build local && tsc -p cli.tsconfig.json && (chmod +x cli.js || true)",
    "prepublishOnly": "npm run build && npm run build-cli",
    "publish": "npm publish --access public",
    "typesafe": "tsc --project tsconfig.typesafe.json",
    "dev": "ace build local && ace sw && vinxi dev",
    "dev-fresh": "rm -rf .ace && npm run dev",
    "bump": "ace build local && ace sw"
  },
  "keywords": [
    "solid",
    "solidjs",
    "ace",
    "acets",
    "acets-team",
    "wizard"
  ],
  "devDependencies": {
    "@acets-team/ace": "^0.6.6",
    "@highlightjs/cdn-assets": "^11.11.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.2.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.0",
    "ag-grid-community": "^34.3.1",
    "chart.js": "^4.5.1",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "solid-js": "^1.9.10",
    "typescript": "^5.9.3",
    "valibot": "^1.1.0",
    "vinxi": "^0.5.8"
  }
}
