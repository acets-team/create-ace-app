  "type": "module",
  "version": "0.0.40",
  "author": "https://github.com/acets-team",
  "homepage": "https://github.com/acets-team/create-ace-app#readme",
  "description": "ðŸ‘‹ Ace is the typesafetiest framework! & npx create-ace-app@latest is a wizard to help us get started developing w/ Ace!",
  "bin": {
    "create-ace-app": "./cli.js"
  },
  "bugs": {
    "url": "https://github.com/acets-team/create-ace-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acets-team/create-ace-app.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build-cli",
    "dev": "ace build local && vinxi dev",
    "build": "ace build prod && vinxi build",
    "publish": "npm publish --access public",
    "typesafe": "tsc --project tsconfig.typesafe.json",
    "build-cli": "ace build local && tsc -p cli.tsconfig.json && (chmod +x cli.js || true)",
    "cloud": "npm run build && npm run typesafe && npm version patch && git push && npm run publish"
  },
  "keywords": [
    "solid",
    "solidjs",
    "ace",
    "acets",
    "acets-team",
    "wizard"
  ],
  "devDependencies": {
    "@acets-team/ace": "^0.0.78",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.7",
    "@types/node": "^24.1.0",
    "solid-js": "^1.9.7",
    "typescript": "^5.8.3",
    "valibot": "^1.1.0",
    "vinxi": "^0.5.8"
  }
}
